{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "functions": {
        "api/**/*.js": {
            "maxDuration": 10
        }
    },
    "env": {
        "WORKSURE_DATABASE_URL": "@worksure-database-url"
    }
}