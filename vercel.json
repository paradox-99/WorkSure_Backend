{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "functions": {
        "index.js": {
            "maxDuration": 30,
            "memory": 1024
        }
    }
}